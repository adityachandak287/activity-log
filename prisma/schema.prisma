datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Activity {
  id        String   @id @default(cuid())
  name      String   @db.VarChar(255)
  count     Int      @db.Int
  timestamp DateTime @default(now())

  @@map("activities")
}
